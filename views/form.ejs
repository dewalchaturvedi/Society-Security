<div class="card" style="width: 18rem; margin-left:40%; background-color:black; color:white; margin-bottom:30px; border-radius:130px; opacity:0.9;">
  <img src="logo.png" class="card-img-top">
  <div class="card-body">
    <p class="card-text" style="font-size:30px;"><b>GATE ENTRY</b></p>
  </div>
</div>
<form action="/results" method="POST" autocomplete="off">
<div style="width:500px; margin-left:33%; opacity:0.8;">
<div class="input-group mb-3">
  <label class="input-group-text" for="inputGroupSelect01">Vehicle Type</label>
  <select name="vehicle" class="form-select" id="inputGroupSelect01">
    <option selected>Choose...</option>
    <option value="Personal Car">Personal Car</option>
    <option value="Cab">Cab</option>
    <option value="Two Wheeler">Two Wheeler</option>
    <option value="Auto Rickshaw">Auto-Rickshaw</option>
    <option value="Commercial">Commercial Vehicle</option>
    <option value="Heavy">Heavy Vehicle</option>
  </select>
</div>

<div class="input-group mb-3">
  <label class="input-group-text" for="inputGroupSelect01">Identity</label>
  <select name="identity" class="form-select" id="inputGroupSelect01">
    <option selected>Choose...</option>
    <option value="Resident">Resident</option>
    <option value="Guest">Guest</option>
    <option value="Society Worker">Society Worker</option>
    <option value="Resident Worker">Resident Worker</option>
    <option value="Shops">Shops</option>
  </select>
</div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Vehicle Number</label>
    <input type="text" name="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Full Name</label>
    <input name="name" type="text" class="form-control" id="exampleInputPassword1">
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Contact Number</label>
    <input name="contact" type="text" class="form-control" id="exampleInputPassword1">
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Tower Number</label>
    <input name="tower" type="text" class="form-control" id="exampleInputPassword1">
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Flat Number</label>
    <input name="flat" type="text" class="form-control" id="exampleInputPassword1">
  </div>
  <div class="input-group mb-3">
  <label class="input-group-text" for="inputGroupSelect01">Intercom</label>
  <select name="intercom" class="form-select" id="inputGroupSelect01">
    <option selected>Choose...</option>
    <option value="Approved">Approved to enter</option>
    <option value="Not Recieved">Call Not Recieved</option>
  </select>
</div>
</div>
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="staticBackdropLabel" style="color:blue;">SECURITY CHECK!</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body" style="color:green;">
        ENTRY DONE!
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
<button type="submit" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
  Submit
</button>
</form>
